        "expo-random": "~13.0.0",
        "expo-secure-store": "~12.0.0",
        "expo-splash-screen": "~0.17.5",
        "expo-status-bar": "~1.4.2",
        "expo-web-browser": "~12.0.0",
        "fast-xml-parser": "^4.0.7",
        "i18next": "^22.0.6",
        "intl": "^1.2.5",
